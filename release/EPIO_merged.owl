        <oboInOwl:created_by>HPO:probinson</oboInOwl:created_by>
        <rdfs:label>Abnormal CSF metabolite level</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/HP_0025613 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/HP_0025613">
        <rdfs:subClassOf rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#focal_non-motor_onset_seizure"/>
        <obo:IAO_0000115>A type of focal-onset seizure characterized by alterations in mood or emotion, or the appearance of altered emotion without the subjective emotion, at seizure onset. These emotional seizures may occur with or without objective clinical signs of a seizure evident to the observer.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/hp.owl"/>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#dateTime">2018-10-07T16:18:24Z</dc:date>
        <oboInOwl:created_by>HPO:probinson</oboInOwl:created_by>
        <oboInOwl:hasDefinition>Focal emotional seizures are characterized by alterations in mood or emotion, or the appearance of altered emotion without the subjective emotion, at seizure onset. These emotional seizures may occur with or without objective clinical signs of a seizure evident to the observer.</oboInOwl:hasDefinition>
        <oboInOwl:hasExactSynonym>emotional</oboInOwl:hasExactSynonym>
        <rdfs:label>Focal emotional seizure</rdfs:label>
        <fromILAE rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromILAE>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://purl.obolibrary.org/obo/HP_0025613"/>
        <owl:annotatedProperty rdf:resource="http://www.geneontology.org/formats/oboInOwl#hasDefinition"/>
        <owl:annotatedTarget>Focal emotional seizures are characterized by alterations in mood or emotion, or the appearance of altered emotion without the subjective emotion, at seizure onset. These emotional seizures may occur with or without objective clinical signs of a seizure evident to the observer.</owl:annotatedTarget>
        <rdfs:isDefinedBy>https://www.epilepsydiagnosis.org/seizure/emotional-overview.html</rdfs:isDefinedBy>
    </owl:Axiom>
    


