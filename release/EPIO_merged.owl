        <obo:creation_date>2010-08-04T01:50:13Z</obo:creation_date>
        <rdfs:label>abdominal morphological measurement</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CMO_0000496 -->

