        <obo:IAO_0000115>Seizures of which initial semiology indicates, or is consistent with, initial activation of only part of one cerebral hemisphere.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/hp.owl"/>
        <oboInOwl:created_by>peter</oboInOwl:created_by>
        <oboInOwl:creation_date>2008-03-31T05:27:00Z</oboInOwl:creation_date>
        <oboInOwl:hasAlternativeId>HP:0002358</oboInOwl:hasAlternativeId>
        <oboInOwl:hasDbXref>MSH:D012640</oboInOwl:hasDbXref>
