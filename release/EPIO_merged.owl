    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CMO_0001970">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CMO_0001232"/>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/cmo.owl"/>
        <obo:created_by>JSmith</obo:created_by>
