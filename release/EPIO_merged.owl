        <rdfs:seeAlso>https://emedicine.medscape.com/article/239191-overview</rdfs:seeAlso>
        <rdfs:seeAlso>https://www.epilepsy.com/learn/professionals/co-existing-disorders/renal-disorders/uremic-encephalopathy</rdfs:seeAlso>
        <fromArticle rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromArticle>
        <fromPubMed rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromPubMed>
