        <oboInOwl:hasDbXref>UMLS:C0039070</oboInOwl:hasDbXref>
        <oboInOwl:hasExactSynonym>Fainting spell</oboInOwl:hasExactSynonym>
        <oboInOwl:hasOBONamespace>human_phenotype</oboInOwl:hasOBONamespace>
        <oboInOwl:hasRelatedSynonym>Syncope and anoxic seizures</oboInOwl:hasRelatedSynonym>
