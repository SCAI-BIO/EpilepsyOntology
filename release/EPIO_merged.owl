        <rdfs:seeAlso>https://www.epilepsy.com/learn/treating-seizures-and-epilepsy/seizure-and-epilepsy-medicines/blood-testing</rdfs:seeAlso>
        <fromArticle rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromArticle>
