    <!-- http://purl.obolibrary.org/obo/HP_0011158 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/HP_0011158">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/HP_0011157"/>
        <obo:IAO_0000115>A type of focal sensory seizure that is characterized by elementary auditory phenomena including buzzing, ringing, drumming or single tones.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/hp.owl"/>
