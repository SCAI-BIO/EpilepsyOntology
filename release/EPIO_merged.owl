    <!-- http://purl.obolibrary.org/obo/HP_0030826 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/HP_0030826">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/HP_0031785"/>
        <obo:IAO_0000115>Tiny, repetitive muscle contractions in the eyelids, causing the appearance of twitching.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/hp.owl"/>
        <oboInOwl:hasDbXref>UMLS:C4280682</oboInOwl:hasDbXref>
