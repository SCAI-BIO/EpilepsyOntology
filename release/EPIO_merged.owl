        <skos:exactMatch rdf:resource="http://linkedlifedata.com/resource/umls/id/CN205631"/>
        <skos:exactMatch rdf:resource="http://www.orpha.net/ORDO/Orphanet_71518"/>
        <fromILAE rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromILAE>
    </owl:Class>
