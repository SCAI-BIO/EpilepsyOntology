        <obo:created_by>mshimoyama</obo:created_by>
        <obo:creation_date>2010-06-10T09:12:29Z</obo:creation_date>
        <obo:def>Total distance around the head of an organism.</obo:def>
        <oboInOwl:hasDbXRef>https://www.ncbi.nlm.nih.gov/pubmed/2384077</oboInOwl:hasDbXRef>
        <rdfs:label>head circumference</rdfs:label>
        <fromESSO rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromESSO>
        <fromPubMed rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromPubMed>
