        <oboInOwl:hasDbXRef>https://www.ncbi.nlm.nih.gov/pubmed/28288363</oboInOwl:hasDbXRef>
        <rdfs:label>blood cytokine measurement</rdfs:label>
        <fromPubMed rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromPubMed>
    </owl:Class>
    


