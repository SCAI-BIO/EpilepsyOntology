    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#corpus_callosotomy -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#corpus_callosotomy">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OMIABIS_0001041"/>
        <oboInOwl:hasDefinition>Corpus callosotomy (CC) has been utilized as a palliative procedure for DRE for more than half a century. Transection is intended to disrupt rapid interhemispheric transcallosal propagation of seizures, particularly from regions of cortex associated with ictal motor manifestations. The target for resection is typically the anterior to mid callosum, with sparing of the posterior fibers in an attempt to mitigate the risks of a sensory disconnection syndrome.</oboInOwl:hasDefinition>
        <oboInOwl:hasExactSynonym>callosotomy</oboInOwl:hasExactSynonym>
        <rdfs:label>corpus callosotomy</rdfs:label>
        <fromPubMed rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromPubMed>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#corpus_callosotomy"/>
        <owl:annotatedProperty rdf:resource="http://www.geneontology.org/formats/oboInOwl#hasDefinition"/>
        <owl:annotatedTarget>Corpus callosotomy (CC) has been utilized as a palliative procedure for DRE for more than half a century. Transection is intended to disrupt rapid interhemispheric transcallosal propagation of seizures, particularly from regions of cortex associated with ictal motor manifestations. The target for resection is typically the anterior to mid callosum, with sparing of the posterior fibers in an attempt to mitigate the risks of a sensory disconnection syndrome.</owl:annotatedTarget>
        <rdfs:isDefinedBy>https://www.ncbi.nlm.nih.gov/pubmed/30806817</rdfs:isDefinedBy>
    </owl:Axiom>
    


