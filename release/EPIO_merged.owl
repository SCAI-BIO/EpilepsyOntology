        <oboInOwl:hasOBONamespace>human_phenotype</oboInOwl:hasOBONamespace>
        <oboInOwl:id>HP:0011150</oboInOwl:id>
        <rdfs:label>Myoclonic absence</rdfs:label>
        <rdfs:seeAlso>https://www.epilepsydiagnosis.org/seizure/awsf-myoclonic-absence-overview.html</rdfs:seeAlso>
        <fromILAE rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromILAE>
