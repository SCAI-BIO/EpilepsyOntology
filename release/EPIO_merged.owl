    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#temporal_intermittent_rythmic_delta_activity -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#temporal_intermittent_rythmic_delta_activity">
