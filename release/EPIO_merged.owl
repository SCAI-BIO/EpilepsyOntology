        <oboInOwl:id>HP:0002521</oboInOwl:id>
        <rdfs:label>Hypsarrhythmia</rdfs:label>
        <rdfs:seeAlso>http://www.case.edu/EpSO.owl#Hypsarrhythmia</rdfs:seeAlso>
        <fromESSO rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromESSO>
        <fromEpSO></fromEpSO>
        <fromPubMed rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromPubMed>
