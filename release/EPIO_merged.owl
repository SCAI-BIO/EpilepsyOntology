        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/HP_0010850"/>
        <obo:IAO_0000115>The presence of complexes of slow spikes and slow waves (&lt;2.5 Hz) in electroencephalography (EEG).</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/hp.owl"/>
        <oboInOwl:created_by>peter</oboInOwl:created_by>
        <oboInOwl:creation_date>2010-07-10T08:18:25Z</oboInOwl:creation_date>
        <oboInOwl:hasDbXref>UMLS:C4023686</oboInOwl:hasDbXref>
