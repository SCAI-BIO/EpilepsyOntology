    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#unknown_onset_seizure -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#unknown_onset_seizure">
        <rdfs:subClassOf rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#seizure_classification"/>
        <oboInOwl:hasDefinition>For everyday purposes seizures are broadly categorized as either generalized or focal in onset, these terms can be used where appropriate, but there are seizures that cannot be categorized in this manner and are classified as having unknown onset. Seizures with unknown onset can be further classified as motor (for example epileptic spasm, tonic-clonic), or non-motor (for example, behavior arrest) in type.</oboInOwl:hasDefinition>
        <rdfs:label>unknown onset seizure</rdfs:label>
        <fromILAE rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromILAE>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#unknown_onset_seizure"/>
        <owl:annotatedProperty rdf:resource="http://www.geneontology.org/formats/oboInOwl#hasDefinition"/>
        <owl:annotatedTarget>For everyday purposes seizures are broadly categorized as either generalized or focal in onset, these terms can be used where appropriate, but there are seizures that cannot be categorized in this manner and are classified as having unknown onset. Seizures with unknown onset can be further classified as motor (for example epileptic spasm, tonic-clonic), or non-motor (for example, behavior arrest) in type.</owl:annotatedTarget>
        <rdfs:isDefinedBy>https://www.epilepsydiagnosis.org/seizure/unknown-onset-groupoverview.html</rdfs:isDefinedBy>
    </owl:Axiom>
    


