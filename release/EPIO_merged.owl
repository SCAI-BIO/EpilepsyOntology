    <!-- http://purl.obolibrary.org/obo/PW_0000007 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PW_0000007">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0007165"/>
        <obo:IAO_0000115>The mitogen -activated protein kinase pathway groups several serine/threonine protein kinases mediated cascades in response to a number of extracellular stimuli.  A characteristic feature of these cascades is the presence of at least three kinases in series leading to the activation of a multifunctional MAP kinase.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/pw.owl"/>
        <oboInOwl:hasDbXRef>https://www.ncbi.nlm.nih.gov/pubmed/26677173</oboInOwl:hasDbXRef>
