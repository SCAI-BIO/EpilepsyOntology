        <obo:created_by>mshimoyama</obo:created_by>
        <obo:creation_date>2011-01-04T02:53:16Z</obo:creation_date>
        <obo:def>The amount of potassium ions in a specified volume of blood.</obo:def>
        <rdfs:label>blood potassium level</rdfs:label>
        <rdfs:seeAlso>https://www.webmd.com/epilepsy/guide/epilepsy-blood-test</rdfs:seeAlso>
        <fromArticle rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromArticle>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CMO_0000502 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CMO_0000502">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CMO_0000063"/>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/cmo.owl"/>
        <obo:created_by>mshimoyama</obo:created_by>
        <obo:creation_date>2011-01-04T03:13:20Z</obo:creation_date>
