        <fromPubMed rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromPubMed>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/PW_0000844 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PW_0000844">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0007165"/>
        <obo:IAO_0000115>Glutamate signaling via the ionotropic AMPA or NMDA and several metabotropic receptors plays important roles in the modulation of synaptic plasticity, long term depression or potentiation, and in learning and memory.</obo:IAO_0000115>
