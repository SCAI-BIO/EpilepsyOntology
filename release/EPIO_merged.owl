    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#rhythmic_temporal_theta_of_drowsiness -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#rhythmic_temporal_theta_of_drowsiness">
        <rdfs:subClassOf rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#rhythmic_benign_variant"/>
        <oboInOwl:hasDbXRef>https://www.ncbi.nlm.nih.gov/books/NBK390352/</oboInOwl:hasDbXRef>
