        <oboInOwl:hasExactSynonym>Abnormal levels of creatine kinase in blood</oboInOwl:hasExactSynonym>
        <rdfs:label>Abnormal circulating creatine kinase concentration</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/HP_0100000 -->

