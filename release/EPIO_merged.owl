        <obo:created_by>mshimoyama</obo:created_by>
        <obo:creation_date>2011-09-21T11:16:38Z</obo:creation_date>
        <rdfs:label>ventilation measurement</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CMO_0000182 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CMO_0000182">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CMO_0000021"/>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/cmo.owl"/>
        <obo:created_by>mshimoyama</obo:created_by>
        <obo:creation_date>2010-06-10T09:12:14Z</obo:creation_date>
        <obo:def>Morphological measurement of the top most or forward most division of an organism&apos;s body usually containing the brain and sense organs.</obo:def>
        <rdfs:label>head morphological measurement</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CMO_0000183 -->

