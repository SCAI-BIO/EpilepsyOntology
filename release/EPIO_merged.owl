        <oboInOWL:hasRelatedSynonym>waist girth</oboInOWL:hasRelatedSynonym>
        <oboInOwl:hasDbXRef>https://www.ncbi.nlm.nih.gov/pubmed/25179745</oboInOwl:hasDbXRef>
        <rdfs:label>waist circumference</rdfs:label>
        <fromESSO rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromESSO>
        <fromPubMed rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromPubMed>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CMO_0000289 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CMO_0000289">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CMO_0000114"/>
