        <owl:annotatedSource rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#unclassified_seizure_onset"/>
        <owl:annotatedProperty rdf:resource="http://www.geneontology.org/formats/oboInOwl#hasDefinition"/>
        <owl:annotatedTarget>A seizure may be unclassified due to inadequate information to allow it to be placed in the focal, generalized or unknown onset categories. This may occur if it was not witnessed at onset, and if results of investigations (such as EEG and imaging) are not yet available.</owl:annotatedTarget>
        <rdfs:isDefinedBy>https://www.epilepsydiagnosis.org/seizure/unknown-onset-groupoverview.html</rdfs:isDefinedBy>
    </owl:Axiom>
    


    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#unknown_epilepsy -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#unknown_epilepsy">
        <rdfs:subClassOf rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#epilepsy_classification"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://bio.scai.fraunhofer.de/ontology/ac113c60_c3d8_48f2_871c_cc34524d844f"/>
                <owl:someValuesFrom rdf:resource="http://purl.obolibrary.org/obo/DOID_1826"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <oboInOwl:hasDefinition>The term &apos;unknown&apos; is used to denote where it is understood that the patient has epilepsy, but it is not possible to denote whether it is focal, generalized, or combined focal and generalized. This may occur due to insufficient information to classify the epilepsy, for example if the EEG is normal/uninformative.</oboInOwl:hasDefinition>
        <rdfs:label>unknown epilepsy</rdfs:label>
        <fromILAE rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromILAE>
