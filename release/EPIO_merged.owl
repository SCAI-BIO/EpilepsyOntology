    <!-- http://purl.obolibrary.org/obo/CMO_0001232 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CMO_0001232">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CMO_0000000"/>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/cmo.owl"/>
        <obo:created_by>JSmith</obo:created_by>
        <obo:creation_date>2013-01-07T13:38:48Z</obo:creation_date>
