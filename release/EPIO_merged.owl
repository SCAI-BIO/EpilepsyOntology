        <rdfs:label>blood calcium level</rdfs:label>
        <rdfs:seeAlso>https://www.webmd.com/epilepsy/guide/epilepsy-blood-test</rdfs:seeAlso>
        <fromArticle rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromArticle>
    </owl:Class>
    


