        <rdfs:label>red blood cell measurement</rdfs:label>
        <rdfs:seeAlso>https://www.webmd.com/epilepsy/guide/epilepsy-blood-test</rdfs:seeAlso>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CMO_0001924 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CMO_0001924">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CMO_0000028"/>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/cmo.owl"/>
        <obo:created_by>JSmith</obo:created_by>
        <obo:creation_date>2013-11-26T11:05:41Z</obo:creation_date>
