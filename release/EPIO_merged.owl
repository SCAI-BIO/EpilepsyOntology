    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#secondary_sensory_area_(parietal_upper_bank_of_the_sylvian_fissure)_seizure -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#secondary_sensory_area_(parietal_upper_bank_of_the_sylvian_fissure)_seizure">
