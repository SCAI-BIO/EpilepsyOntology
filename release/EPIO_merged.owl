    <!-- http://purl.obolibrary.org/obo/CMO_0000921 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CMO_0000921">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CMO_0000036"/>
