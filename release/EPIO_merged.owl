    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#extensor_spasm"/>
        <owl:annotatedProperty rdf:resource="http://www.geneontology.org/formats/oboInOwl#hasDefinition"/>
        <owl:annotatedTarget>Extensor spasticity is an involuntary straightening of the legs. Extensor spasticity involves the quadriceps (muscles on the front of the upper leg) and the adductors (inner thigh muscles).</owl:annotatedTarget>
        <rdfs:isDefinedBy>https://www.aans.org/Patients/Neurosurgical-Conditions-and-Treatments/Spasticity</rdfs:isDefinedBy>
    </owl:Axiom>
    


