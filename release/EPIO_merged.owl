    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#GABA-B_receptor_antibody -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#GABA-B_receptor_antibody">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MONDO_0100029"/>
        <oboInOwl:hasDefinition>GABA-B receptor antibodies may present with limbic encephalitis and are most commonly found in adults with small cell lung cancer. Diagnosis is supported by identification of GABA-B receptor antibody in serum.</oboInOwl:hasDefinition>
