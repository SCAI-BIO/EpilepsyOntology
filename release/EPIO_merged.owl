    <!-- http://purl.obolibrary.org/obo/MONDO_0019197 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/MONDO_0019197">
        <rdfs:subClassOf rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#metabolic_etiology"/>
        <obo:IAO_0000115>Folinic acid-responsive seizures is a very rare neonatal epileptic encephalopathy disorder characterized clinically by myoclonic and clonic, or clonic seizures associated with apnea occurring several hours to 5 days after birth and responding to folinic acid.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/mondo.owl"/>
        <oboInOwl:hasDbXref>ICD10:G40.3</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>Orphanet:79097</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>SCTID:717276003</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>UMLS:CN205780</oboInOwl:hasDbXref>
        <oboInOwl:hasDefinition>This metabolic disorder is an allelic condition to pyridoxine dependent epilepsy with similar biochemical markers. Individuals may have a partial pyridoxine response, then may require co-therapy with folinic acid. Research is underway into the mechanism for folinic acid response. In CSF studies of biogenic amines, an unknown peak (peak X) is seen.</oboInOwl:hasDefinition>
        <oboInOwl:hasExactSynonym>Folinic acid responsive seizures</oboInOwl:hasExactSynonym>
        <oboInOwl:id>MONDO:0019197</oboInOwl:id>
        <oboInOwl:inSubset rdf:resource="http://purl.oboInOwllibrary.org/oboInOwl/mondo#ordo_disease"/>
