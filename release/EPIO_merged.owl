        <owl:annotatedSource rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#ducky_mouse"/>
        <owl:annotatedProperty rdf:resource="http://www.geneontology.org/formats/oboInOwl#hasDefinition"/>
        <owl:annotatedTarget>Mouse mutant ducky, a model for absence epilepsy, is characterized by spike-wave seizures and ataxia.</owl:annotatedTarget>
        <rdfs:isDefinedBy>https://www.ncbi.nlm.nih.gov/pubmed/11487633</rdfs:isDefinedBy>
