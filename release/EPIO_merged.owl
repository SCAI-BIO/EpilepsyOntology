        <oboInOwl:hasDbXref>FMA:83686</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>NCIT:C13197</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>NLX:28443</oboInOwl:hasDbXref>
        <oboInOwl:hasExactSynonym>nervous system nucleus</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym>neuraxis nucleus</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym>neuronal nucleus</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym>nucleus of CNS</oboInOwl:hasExactSynonym>
        <oboInOwl:hasOBONamespace>uberon</oboInOwl:hasOBONamespace>
        <oboInOwl:hasRelatedSynonym>nucleus of neuraxis</oboInOwl:hasRelatedSynonym>
