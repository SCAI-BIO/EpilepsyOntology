    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#episodic_ataxia_type-2_model -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#episodic_ataxia_type-2_model">
