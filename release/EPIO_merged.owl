    <owl:Class rdf:about="http://purl.obolibrary.org/obo/HP_0002315">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/HP_0012638"/>
        <obo:IAO_0000115>Cephalgia, or pain sensed in various parts of the head, not confined to the area of distribution of any nerve.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/hp.owl"/>
        <oboInOwl:hasAlternativeId>HP:0000266</oboInOwl:hasAlternativeId>
        <oboInOwl:hasAlternativeId>HP:0001354</oboInOwl:hasAlternativeId>
