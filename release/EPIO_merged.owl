    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#post-ictal -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#post-ictal">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PATO_0001912"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://bio.scai.fraunhofer.de/ontology/cac13978_e4e8_4240_907f_80ddfd3af981"/>
