    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#quality_of_life_in_epilepsy_inventory_-_89 -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#quality_of_life_in_epilepsy_inventory_-_89">
