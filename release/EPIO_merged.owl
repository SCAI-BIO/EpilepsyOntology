    <!-- http://purl.obolibrary.org/obo/HP_0002597 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/HP_0002597">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/HP_0001626"/>
        <obo:IAO_0000115>An abnormality of the vasculature.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/hp.owl"/>
        <oboInOwl:hasDbXref>UMLS:C0241657</oboInOwl:hasDbXref>
