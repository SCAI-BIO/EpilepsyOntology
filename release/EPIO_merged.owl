        <oboInOWL:xref rdf:resource="http://purl.obolibrary.org/obo/CHEBI_24996"/>
        <oboInOwl:hasDbXRef>https://www.ncbi.nlm.nih.gov/pubmed/29110774</oboInOwl:hasDbXRef>
        <rdfs:label>blood lactate level</rdfs:label>
        <fromPubMed rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromPubMed>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CMO_0001356 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CMO_0001356">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CMO_0000036"/>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/cmo.owl"/>
        <obo:created_by>JSmith</obo:created_by>
        <obo:creation_date>2013-01-10T17:31:25Z</obo:creation_date>
