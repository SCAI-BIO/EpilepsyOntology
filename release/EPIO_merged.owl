        <rdfs:label>pulse</rdfs:label>
        <fromESSO rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromESSO>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CMO_0000296 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CMO_0000296">
