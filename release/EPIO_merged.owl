    <!-- http://purl.obolibrary.org/obo/PW_0000180 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PW_0000180">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/GO_0007165"/>
        <obo:IAO_0000115>mTOR signaling pathway regulates cellular processes such as translation, ribosome biogenesis, cell growth and autophagy and is regulated or responds to growth factors, energy metabolites and/or levels of nutrients.</obo:IAO_0000115>
