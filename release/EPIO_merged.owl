    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#reading_epilepsy -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#reading_epilepsy">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DOID_2548"/>
        <oboInOwl:hasDefinition>Reading epilepsy is characterized by seizures that start between 12 and 19 years of age. There is a male predominance (1.8M:F). Seizures are elicited by reading (aloud or silently). Prognosis is good as seizures are usually minor and can be avoided through reducing exposure to the stimulus.</oboInOwl:hasDefinition>
        <oboInOwl:hasRelatedSynonym>Primary Reading Epilepsy</oboInOwl:hasRelatedSynonym>
        <rdfs:label>reading epilepsy</rdfs:label>
        <rdfs:seeAlso>http://www.case.edu/EpSO.owl#PrimaryReadingEpilepsy</rdfs:seeAlso>
        <fromEpSO></fromEpSO>
        <fromILAE rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromILAE>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#reading_epilepsy"/>
        <owl:annotatedProperty rdf:resource="http://www.geneontology.org/formats/oboInOwl#hasDefinition"/>
        <owl:annotatedTarget>Reading epilepsy is characterized by seizures that start between 12 and 19 years of age. There is a male predominance (1.8M:F). Seizures are elicited by reading (aloud or silently). Prognosis is good as seizures are usually minor and can be avoided through reducing exposure to the stimulus.</owl:annotatedTarget>
        <rdfs:isDefinedBy>https://www.epilepsydiagnosis.org/syndrome/reflex-epilepsies-overview.html</rdfs:isDefinedBy>
    </owl:Axiom>
    


