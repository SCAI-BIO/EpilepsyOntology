        <obo:created_by>mshimoyama</obo:created_by>
        <obo:creation_date>2010-06-17T10:30:36Z</obo:creation_date>
        <obo:def>Any quantification of a morphological or physiological parameter of one or more cells.  A cell is a membrane-enclosed protoplasmic mass constituting the smallest structural unit of an organism that is capable of independent functioning.</obo:def>
        <rdfs:label>cell measurement</rdfs:label>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CMO_0000242 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CMO_0000242">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CMO_0000296"/>
