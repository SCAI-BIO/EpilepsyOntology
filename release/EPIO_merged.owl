        <oboInOWL:hasExactSynonym>haemoglobin measurement</oboInOWL:hasExactSynonym>
        <rdfs:label>hemoglobin measurement</rdfs:label>
        <rdfs:seeAlso>https://www.webmd.com/epilepsy/guide/epilepsy-blood-test</rdfs:seeAlso>
        <fromArticle rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromArticle>
    </owl:Class>
    


    <!-- http://purl.obolibrary.org/obo/CMO_0000670 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/CMO_0000670">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/CMO_0000001"/>
