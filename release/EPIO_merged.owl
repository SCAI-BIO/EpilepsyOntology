    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DOID_3328">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/DOID_1826"/>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/doid.owl"/>
