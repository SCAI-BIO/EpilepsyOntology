        <CommentOnDefinition>Modified definition</CommentOnDefinition>
    </owl:Class>
    


    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#convulsive_status_epilepticus -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#convulsive_status_epilepticus">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/HP_0032658"/>
        <oboInOwl:hasDbXRef>https://www.ncbi.nlm.nih.gov/pubmed/26920416</oboInOwl:hasDbXRef>
        <oboInOwl:hasDefinition>Convulsive status epilepticus is defined as a convulsive seizure lasting more than 5 min or consecutive seizures without recovery of consciousness.</oboInOwl:hasDefinition>
