        <owl:annotatedProperty rdf:resource="http://www.geneontology.org/formats/oboInOwl#hasDefinition"/>
        <owl:annotatedTarget>Diaphragmatic breathing is a type of a breathing exercise that helps strengthen your diaphragm, an important muscle that helps you breathe. This breathing exercise is also sometimes called belly breathing or abdominal breathing</owl:annotatedTarget>
        <rdfs:isDefinedBy>https://www.healthline.com/health/diaphragmatic-breathing</rdfs:isDefinedBy>
    </owl:Axiom>
    


    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#dominant_parieto-temporal_region_seizure -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#dominant_parieto-temporal_region_seizure">
