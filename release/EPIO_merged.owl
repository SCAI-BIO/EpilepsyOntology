    <!-- http://purl.obolibrary.org/obo/DOID_1932 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/DOID_1932">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MONDO_0019040"/>
        <obo:IAO_0000115>A syndrome that is characterized by delayed development, intellectual disability, severe speech impairment, and problems with movement and balance.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/doid.owl"/>
