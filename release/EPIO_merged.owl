        <oboInOwl:hasDbXref>SNOMEDCT_US:189198006</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>SNOMEDCT_US:42365007</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>UMLS:C0270846</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>UMLS:C1836509</oboInOwl:hasDbXref>
        <oboInOwl:hasExactSynonym>Astatic seizure</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym>Astatic seizures</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym>Atonic seizures</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym>Drop attacks</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym>Drop seizures</oboInOwl:hasExactSynonym>
        <oboInOwl:hasExactSynonym>Hypotonic seizure</oboInOwl:hasExactSynonym>
        <oboInOwl:hasOBONamespace>human_phenotype</oboInOwl:hasOBONamespace>
        <oboInOwl:hasRelatedSynonym>Hypotonic seizures</oboInOwl:hasRelatedSynonym>
        <oboInOwl:hasRelatedSynonym>Sudden loss of muscle tone</oboInOwl:hasRelatedSynonym>
        <oboInOwl:id>HP:0010819</oboInOwl:id>
        <rdfs:comment>This term describes the initial semiology of the seizure without specifying whether the onset is focal or generalized. Thus it can be used for coding atonic seizures when the onset is not known.</rdfs:comment>
        <rdfs:label>Atonic seizure</rdfs:label>
        <rdfs:seeAlso>http://www.case.edu/EpSO.owl#AtonicSeizure</rdfs:seeAlso>
        <rdfs:seeAlso>https://www.epilepsydiagnosis.org/seizure/atonic-overview.html</rdfs:seeAlso>
        <fromILAE rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromILAE>
