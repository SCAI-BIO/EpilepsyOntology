        <oboInOwl:id>PW:0000844</oboInOwl:id>
        <rdfs:label>glutamate signaling pathway</rdfs:label>
        <fromPubMed rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromPubMed>
