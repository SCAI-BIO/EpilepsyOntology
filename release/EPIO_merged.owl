    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#lateral_/_neocortical_temporal_lobe_seizure">
        <rdfs:subClassOf rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#temporal_lobe_seizure"/>
        <oboInOwl:hasDbXRef>https://www.ncbi.nlm.nih.gov/pubmed/15562304</oboInOwl:hasDbXRef>
        <oboInOwl:hasDefinition>Lateral temporal lobe seizures may have an initial focal seizure with auditory or vertiginous features. The focal sensory auditory seizure is usually a basic sound such as buzzing or ringing (rather than formed speech). If the sound is heard in only one ear it suggests the seizure is in the contralateral hemisphere</oboInOwl:hasDefinition>
        <rdfs:label>lateral / neocortical temporal lobe seizure</rdfs:label>
        <rdfs:seeAlso>http://www.medlink.com/article/neocortical_temporal_lobe_seizures</rdfs:seeAlso>
        <fromILAE rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromILAE>
        <fromPubMed rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromPubMed>
    </owl:Class>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="https://bio.scai.fraunhofer.de/ontology/epilepsy#lateral_/_neocortical_temporal_lobe_seizure"/>
        <owl:annotatedProperty rdf:resource="http://www.geneontology.org/formats/oboInOwl#hasDefinition"/>
        <owl:annotatedTarget>Lateral temporal lobe seizures may have an initial focal seizure with auditory or vertiginous features. The focal sensory auditory seizure is usually a basic sound such as buzzing or ringing (rather than formed speech). If the sound is heard in only one ear it suggests the seizure is in the contralateral hemisphere</owl:annotatedTarget>
        <rdfs:isDefinedBy>https://www.epilepsydiagnosis.org/seizure/temporal-overview.html</rdfs:isDefinedBy>
    </owl:Axiom>
    


