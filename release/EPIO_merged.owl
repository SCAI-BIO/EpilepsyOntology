    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#elementary_partial_seizure -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#elementary_partial_seizure">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/SYMP_0000124"/>
        <oboInOwl:hasDefinition>Partial seizures with elementary motor symptoms, formerly called focal motor seizures, typically consist of rhythmic jerking (clonic movement) of a body region that may be limited to one finger or extend to an entire side of the body</oboInOwl:hasDefinition>
        <rdfs:label>elementary partial seizure</rdfs:label>
        <fromArticle rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromArticle>
