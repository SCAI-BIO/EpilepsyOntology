        <oboInOwl:hasDbXref>MSH:D013036</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>SNOMEDCT_US:28055006</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>UMLS:C0684276</oboInOwl:hasDbXref>
