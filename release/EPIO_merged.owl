    <!-- http://purl.obolibrary.org/obo/PW_0000234 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/PW_0000234">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/PW_0000023"/>
        <obo:IAO_0000115>The innate immune response - a universal and ancient form of host defense against infection - is a first line of response to various pathogens and also to damaged cells. It plays a role in stimulating adaptive immunity; molecules generated during innate responses act as second signals that can impact on both the magnitude and the nature of the adaptive response.</obo:IAO_0000115>
        <obo:IAO_0000412 rdf:resource="http://purl.obolibrary.org/obo/pw.owl"/>
        <oboInOwl:hasDbXRef>https://www.ncbi.nlm.nih.gov/pubmed/26260962</oboInOwl:hasDbXRef>
        <oboInOwl:hasOBONamespace>pathway</oboInOwl:hasOBONamespace>
        <oboInOwl:hasRelatedSynonym>innate immunity</oboInOwl:hasRelatedSynonym>
        <oboInOwl:id>PW:0000234</oboInOwl:id>
        <rdfs:label>innate immune response pathway</rdfs:label>
        <fromPubMed rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</fromPubMed>
    </owl:Class>
    


