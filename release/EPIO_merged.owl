        <oboInOwl:hasDbXref>GARD:6082</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>MESH:C535362</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>NCI:C74983</oboInOwl:hasDbXref>
        <oboInOwl:hasDbXref>OMIM:607872</oboInOwl:hasDbXref>
