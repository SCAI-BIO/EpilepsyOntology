    <!-- https://bio.scai.fraunhofer.de/ontology/epilepsy#trigger_factor_for_seizure -->

    <owl:Class rdf:about="https://bio.scai.fraunhofer.de/ontology/epilepsy#trigger_factor_for_seizure">
